<div class="scroll" style="height:90%">
    @if(listings != null)
    {
        int i = 1;
        @foreach (var listing in listings)
        {
            <SimilarListing ListDetails=@listing></SimilarListing>
            i++;
        }
    }
</div>

@code {
    List<HouseListing> listings;

    HttpClient httpClient = new HttpClient();

    protected override async Task OnInitializedAsync()
    {
        listings = await httpClient.GetFromJsonAsync<List<HouseListing>>("http://localhost:5211/listingsLike.json");
    }
}